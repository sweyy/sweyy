class Solution {
    public int[] twoSum(int[] nums, int target) {
        int res[] = new int[2];
        outerloop;
        for(int i=0;i<nums.length;i++){
            int temp=target;
            temp-=nums[i];
            for(int j=i+1;j<nums.length;j++){
                if(temp==nums[j]){
                    res[0]=i;
                    res[1]=j;
                    break outerloop;
                }
            }
        }
        return res;
    }
}
